        def ret():
            print('{} ¬°Hola Alex! :)'.format(self.on))
        print('{} Encendiendo dispositivo...'.format(self.off)) 
        t = threading.Timer(2.5, ret)
        t.start()
        pass
      

    def power_off(self):
        return '{} ¬°ADIOS!...'.format(self.off)

