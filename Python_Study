'''class MobilePhone:
    pass
    apps = ['meta', 'uber', 'navigator', 'camera app', 'calculator', 'calls', 'sms', 'configuration', 'photo editor']
    on = 1
    off = 0

    def __init__(self, manufacturer, screen_size, num_cores):
