    vec2 = int(input(f'Digite el {j} elemento del vector B: '))
    B[1].append(vec2)
    
print(A)
print(B)
c0 = A[0][0] * B[0][0] + A[0][1] * B[1][0]
c1 = A[0][0] * B[0][1] + A[0][1] * B[1][1]
c2 = A[1][0] * B[0][0] + A[1][1] * B[1][0]
c3 = A[1][0] * B[0][1] + A[1][1] * B[1][1]

